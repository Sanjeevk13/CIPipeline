pipeline {
    agent any

    stages {
        stage ('Clean workspace') {
  		steps {
    		cleanWs()
  		}
	}
	stage ('Git Checkout') {
  steps {
      git branch: 'master', url: 'https://github.com/Sanjeevk13/CIPipeline.git'
    }
  }
  stage('Restore packages') {
  steps {
    bat "dotnet restore ${workspace} "C:\Users\sanjeev.kumar11\.jenkins\workspace\pipelineone" "./DemoApp.sln"
    
  }
}
  
  
    }
        
        
 post {
        success{
            
            echo 'Build successful'
        }
        failure {
            echo 'Build failed'
            slackSend color: 'bad', message: 'your GoRewards Web build has failed'
        }
    }
}
